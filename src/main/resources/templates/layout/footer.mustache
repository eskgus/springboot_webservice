    <!-- bootstrap.js는 제이쿼리 필수라 bootstrap보다 먼저 호출 (bootstrap.js가 제이쿼리에 의존) -->
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <!-- bootsrtrap (js) 라이브러리 -->
    <!-- js는 용량 클수록 body 실행 늦어져서 body 하단에 두고 화면 다 그려지면 호출 -->
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>

    <!-- index.js 추가 -->
    <!-- 스프링 부트에서 기본적으로 src/main/resources/static에 있는 js, css, image 등 정적 파일들은 URL에서 /로 설정됨 -->
    <script src="/js/app/index.js"></script>
</body>
</html>